{
  "name": "learn-anything",
  "scripts": {
    "dev": "bun run web",
    "web": "cd web && bun dev",
    "setup": "bun cmd.ts setup",
    "seed": "bun --watch cli/seed.ts",
    "cli": "bun --watch cli/run.ts",
    "state": "bun --watch cmd.ts jazz-state",
    "edgedb:migrate": "edgedb migration create && edgedb migration apply && npx @edgedb/generate edgeql-js"
  },
  "workspaces": [
    "web"
  ],
  "dependencies": {
    "jazz-nodejs": "^0.7.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "bun-types": "^1.1.15",
    "typescript": "^5.4.5"
  },
  "prettier": {
    "semi": false,
    "useTabs": true
  },
  "license": "MIT"
}
